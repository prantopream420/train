<?xml version="1.0" encoding="UTF-8"?>
<?language javascript?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="283.0" prefWidth="324.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.train.Login">
    <children>
        <Label layoutX="52.0" layoutY="32.0" text="Username" />
        <TextField fx:id="username" layoutX="120.0" layoutY="32.0" promptText="enter your name" />
        <Label layoutX="52.0" layoutY="64.0" text="Password" />
        <PasswordField fx:id="password" layoutX="120.0" layoutY="64.0" promptText="enter your password" />
        <Button layoutX="120.0" layoutY="96.0" onAction="onLoginClick" text="Login" />
        <Hyperlink layoutX="52.0" layoutY="96.0" onAction="switchToReg" text="Register" />
      <Text layoutX="110.0" layoutY="200.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Railway ticket Login" />
    </children>
</AnchorPane>
